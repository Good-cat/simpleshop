<!DOCTYPE html>
<!--[if IE 8]> <html lang="en" class="ie8 no-js"> <![endif]-->
<!--[if IE 9]> <html lang="en" class="ie9 no-js"> <![endif]-->
<!--[if !IE]><!-->
<html class="no-js" lang="en"><!--<![endif]-->
    <head>
        <meta http-equiv="content-type" content="text/html; charset=UTF-8">
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta content="width=device-width, initial-scale=1.0" name="viewport">
        <meta content="{% block description %}{% endblock description %}" name="description">
        <meta content="{% block keywords %}{% endblock keywords %}" name="keywords">
        <title>{% block title %}Carwest!{% endblock title%}</title>
        {% block stylesheets %}
            <link rel="stylesheet" href="{{ asset('bundles/app/css/bootstrap.css') }}"/>
            <link rel="stylesheet" href="{{ asset('bundles/app/css/bootstrap-modal.css') }}"/>
            <link rel="stylesheet" href="{{ asset('bundles/app/css/style_n.css') }}"/>
            <link rel="stylesheet" href="{{ asset('bundles/app/css/css.css') }}"/>
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>

        {{ render(controller('AppBundle:MainPage:getHeader')) }}
        {% block body %}{% endblock %}
        {% block javascripts %}
            <script type="text/javascript" src="{{ asset('bundles/app/js/vendor/jquery.js') }}"></script>
            <script type="text/javascript" src="{{ asset('bundles/app/js/vendor/bootstrap.js') }}"></script>
            <script type="text/javascript" src="{{ asset('bundles/app/js/vendor/bootstrap-modal.js') }}"></script>
        {% endblock %}
    </body>
</html>
