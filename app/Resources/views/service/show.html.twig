{% extends '::base.html.twig' %}
{% block body %}
    <div class="container">
        <h2>
            {{ service.getName }}
        </h2>
        <div class="service_annotation">
            {{ service.getDescription|raw() }}
        </div>
        <a href="http://vkontakte.ru/share.php?url={{ app.request.getHost }}{{ app.request.getPathInfo }}" target="_blank">Поделиться ВКонтакте</a>
        <div class="fb-like" data-href="https://developers.facebook.com/docs/plugins/" data-layout="button" data-action="like" data-show-faces="false" data-share="false"></div>
    </div>

{% endblock body %}